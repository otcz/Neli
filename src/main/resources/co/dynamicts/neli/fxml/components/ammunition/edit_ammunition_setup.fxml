                            <TextfieldTitle fx:id="type"/>
                            <TextfieldTitle fx:id="diameter"/>
                            <TextfieldTitle fx:id="acceleration"/>
                        </children>
                    </HBox>
                    <HBox fx:id="projectilInfo" spacing="12.5">
                        <children>
                            <TextfieldTitle fx:id="zones"/>
                            <TextfieldTitle fx:id="projectileWeight"/>
