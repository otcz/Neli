<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<fx:root alignment="TOP_CENTER" prefWidth="150.0" styleClass="flying-time" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1">
    <Label fx:id="titleLabel" styleClass="title" text="Flying Time" />
    <Label fx:id="valueLabel" styleClass="value" text="00:00:00">
        <VBox.margin>
            <Insets top="4.0" />
        </VBox.margin>
    </Label>
   <padding>
       <Insets bottom="4.0" top="4.0" />
   </padding>
</fx:root>
