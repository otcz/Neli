<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<fx:root maxWidth="-Infinity" minWidth="-Infinity" styleClass="data-field, spacing-7" type="javafx.scene.layout.Pane" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <children>

        <StackPane layoutX="1.0" styleClass="icon-container">
            <children>
                <Region fx:id="iconRegion" styleClass="icon-region">
                    <shape>
                        <SVGPath fx:id="icon" />
                    </shape>
                </Region>
            </children>
        </StackPane>
        <Text fx:id="title" layoutX="23.0" layoutY="12.0" styleClass="align-left, font-12, value, din-regular, white-two"  wrappingWidth="177.0" />
        <Text fx:id="subtitle" layoutX="23.0" layoutY="27.0" styleClass="align-left, font-12, value, din-regular, white-two"  textAlignment="LEFT" wrappingWidth="74.0" />
        <Text fx:id="measure" layoutX="99.0" layoutY="27.0" strokeLineJoin="ROUND" styleClass="align-right, font-14, value, din-regular, white-two, din-bold" text="-000.00 m" textAlignment="RIGHT" wrappingWidth="98.0">
        </Text>


    </children>
    <stylesheets>
        <URL value="@../../../css/variables.css" />
        <URL value="@../../../css/general.css" />
        <URL value="@../../../css/home/data_field.css" />
    </stylesheets>
</fx:root>
